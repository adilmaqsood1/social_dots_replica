{
    "builds": [{
      "src": "socialdots/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "socialdots/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "socialdots.settings"
    }
}